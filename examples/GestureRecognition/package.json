{
  "name": "GestureRecognition",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "npx jest --verbose",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --coverage",
    "test:e2e:ios": "detox test -c ios.sim.debug",
    "test:e2e:ios:build": "detox build",
    "test:e2e:android": "detox test -c android.emu.debug",
    "test:update": "npm test -- -u",
    "ios": "react-native run-ios --simulator \"iPhone X\"",
    "device": "react-native run-ios --device",
    "android": "react-native run-android",
    "androidsim": "~/Library/Android/sdk/emulator/emulator -avd Pixel_2_XL_API_28 &",
    "metro": "npm start -- --reset-cache"
  },
  "dependencies": {
    "react": "16.6.3",
    "react-native": "0.58.6",
    "react-native-camera": "^2.3.0",
    "react-native-tensorflow-lite": "^1.0.6"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.5.0",
    "jest": "24.5.0",
    "metro-react-native-babel-preset": "0.53.1",
    "react-test-renderer": "16.6.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
